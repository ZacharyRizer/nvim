{
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "colors.filetypes": ["*"],
  "diagnostic.floatConfig": { "border": true, "rounded": true },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "floatFactory.floatConfig": { "border": true, "rounded": true },
  "git.addedSign.hlGroup": "GitSignsAdd",
  "git.changeRemovedSign.hlGroup": "GitSignsChange",
  "git.changedSign.hlGroup": "GitSignsChange",
  "git.floatConfig": { "border": true, "rounded": true },
  "git.removedSign.hlGroup": "GitSignsDelete",
  "git.topRemovedSign.hlGroup": "GitSignsDelete",
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "hover.floatConfig": { "border": true, "rounded": true },
  "html.autoClosingTags": false,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k"
  },
  "Lua.hint.enable": false,
  "Lua.telemetry.enable": true,
  "notification.timeout": 5000,
  "prettier.disableLanguages": ["haskell"],
  "prettier.tabWidth": 2,
  "signature.floatConfig": { "border": true, "rounded": true },
  "suggest.floatConfig": { "border": true, "rounded": true },
  "suggest.noselect": true,
  "suggest.removeDuplicateItems": true,
  "sumneko-lua.enableNvimLuaDev": true
}
