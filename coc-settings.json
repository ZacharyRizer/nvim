{
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSave": true,
  "codeLens.enable": true,
  "colors.enable": true,
  "diagnostic.errorSign": " ",
  "diagnostic.hintSign": "󰠠 ",
  "diagnostic.infoSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.floatConfig": { "border": true, "rounded": true },
  "floatFactory.floatConfig": { "border": true, "rounded": true },
  "git.addedSign.hlGroup": "GitSignsAdd",
  "git.changeRemovedSign.hlGroup": "GitSignsChange",
  "git.changedSign.hlGroup": "GitSignsChange",
  "git.floatConfig": { "border": true, "rounded": true },
  "git.removedSign.hlGroup": "GitSignsDelete",
  "git.topRemovedSign.hlGroup": "GitSignsDelete",
  "hover.floatConfig": { "border": true, "rounded": true },
  "html.autoClosingTags": false,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["hs", "lhs", "haskell", "lhaskell"],
      "settings": {
        "haskell": {
          "formattingProvider": "fourmolu"
        }
      }
    }
  },
  "list.floatPreview": true,
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k"
  },
  "Lua.diagnostics.globals": ["vim"],
  "notification.timeout": 5000,
  "prettier.disableLanguages": ["haskell"],
  "prettier.tabWidth": 2,
  "python.formatting.blackPath": "/Users/zrizer/.pyenv/shims/black",
  "python.formatting.provider": "black",
  "signature.floatConfig": { "border": true, "rounded": true },
  "suggest.floatConfig": { "border": true, "rounded": true },
  "suggest.noselect": true,
  "suggest.removeDuplicateItems": true
}
